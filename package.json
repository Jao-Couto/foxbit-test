{
	"name": "foxbit-test",
	"version": "1.0.0",
	"description": "Teste para Foxbit",
	"main": "dist/index.js",
	"scripts": {
		"test": "jest",
		"start": "node dist/index.js",
		"build": "tsc"
	},
	"author": "Jo√£o Couto <joao.couto1228@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"typescript": "^5.3.2"
	},
	"devDependencies": {
		"@types/jest": "^29.5.10",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"moduleNameMapper": {
			"^src/(.*)$": "<rootDir>/$1"
		}
	}
}
